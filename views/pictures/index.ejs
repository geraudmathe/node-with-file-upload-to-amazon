

<div class="row">
  <h2 class="col l12">All Pictures</h2>
  <% pictures.forEach(picture =>{ %>
    <div class="col s12 m4">
      <div class="card">
        <div class="card-image waves-effect waves-block waves-light">
          <img class="activator" src="<%= picture.url %>">
        </div>
        <div class="card-content">
          <span class="card-title activator grey-text text-darken-4"><%= picture.title%><i class="material-icons right">more_vert</i></span>
          <p>Click on the title to see all the details about the picture</p>
        </div>
        <div class="card-reveal">
          <%= picture %>
        </div>
      </div>
    </div>

  <% }); %>
</div>
